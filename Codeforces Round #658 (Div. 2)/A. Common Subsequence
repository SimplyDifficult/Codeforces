#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define fo(i,n) for(ll i=0;i<n;i++)
#define fo1(i,n) for(ll i=1;i<=n;i++)
ll N=100001;
ll mod=1000000007;

ll t,n,x,k,m;
string s;
int flag=0;



int main(){

	#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
    #endif
 	
 	cin>>t;
 	while(t--){
 		cin>>n>>m;
 		ll a[n],b[m];
 		map<ll,ll>a1;
 		map<ll,ll>b1;
 		fo(i,n){
 			cin>>a[i];
 			a1.insert({a[i],1});
 		}	
 		fo(i,m){
 			cin>>b[i];
 			b1.insert({b[i],1});
 		}
 		vector<ll>v;
 		fo(i,n){
 			if(b1.find(a[i])!=b1.end())
 				v.push_back(a[i]);
 		}	
 		if(v.size()==0)cout<<"NO"<<endl;
 		else {
 		    cout<<"YES"<<endl;
 		    cout<<1<<" "<<v[0]<<endl;
 		}    
 	}


    cerr << "Time : " << 1000 * ((double)clock()) / (double)CLOCKS_PER_SEC << "ms\n";
    return 0;
}
