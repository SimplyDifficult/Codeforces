#include <iostream>
#include <bits/stdc++.h> 

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;
        int n=s.size();
        int countp=0,countr=0,counts=0;
        for(int i=0;i<n;i++){
            if(s[i]=='P')countp++;
            if(s[i]=='R')countr++;
            if(s[i]=='S')counts++;
        }
        int m=counts;
        m=max(m,countr);
        m=max(m,countp);
        if(m==counts){
            for(int i=1;i<=n;i++){
                cout<<"R";
            }
        }
        else if(m==countr){
            for(int i=1;i<=n;i++){
                cout<<"P";
            }
        }
        else{
            for(int i=1;i<=n;i++){
                cout<<"S";
            }
        }
        cout<<endl;
    }
        
    
    return 0;
}
