#include <iostream>
#include <bits/stdc++.h> 

using namespace std;

int main()
{
  	int t;
  	cin>>t;
  	while(t--){
  		int n;
  		cin>>n;
  		int a[n+1];
  		for(int i=1;i<=n;i++){
  			cin>>a[i];
  		}
  		int k=0;
  		for(int i=2;i<n;i++){
  			if(a[i]>a[i-1] && a[i]>a[i+1]){
  				cout<<"YES"<<endl;
  				cout<<i-1<<" "<<i<<" "<<i+1<<endl;
  				k=1;
  				break;
  			}
  		}
  		if(k==0)
  			cout<<"NO"<<endl;
  	}
    return 0;
}
