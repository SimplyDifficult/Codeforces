#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define fo(i,n) for(int i=0;i<n;i++)

ll t,n,x,k;

int main(){

	#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
    #endif
 	
 	cin>>t;
 	while(t--){
 		cin>>n>>x;
 		ll a[n];
 		fo(i,n)cin>>a[i];
 		int ans=0,curr=1;
 		sort(a,a+n,greater<int>());
 		fo(i,n){
 			if(a[i] *curr>=x){
 				ans++;
 				curr=0;
 			}
 			curr++;
 		}
 		cout<<ans<<endl;
 	}


    cerr << "Time : " << 1000 * ((double)clock()) / (double)CLOCKS_PER_SEC << "ms\n";
    return 0;
}
