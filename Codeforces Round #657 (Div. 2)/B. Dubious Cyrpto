    #include <iostream>
    #include <bits/stdc++.h> 
     
    using namespace std;
     
    int main() {
    	
    	int t;
    	cin>>t;
    	while(t--){
    		long long l,r,m;
    		cin>>l>>r>>m;
    		long long left=m+l-r;
    		long long right=m+r-l;
    		long long a,b,c;
    		for(a=l;a<=r;a++){
    			long long k=m%a;
    			if(k+r-l>=a){
    				c=r;
    				b=r-a+k;
    				cout<<a<<" "<<b<<" "<<c<<endl;
    				break;
    			}
    			else if(k-r+l<=0 && m/a>0){
    				b=r;
    				c=r-k;
    				cout<<a<<" "<<b<<" "<<c<<endl;
    				break;
    			}
    		}
    	}
    	return 0;
    }
